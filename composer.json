{
    "name": "consilience/l5-api-basic-auth",
    "description": "Stateless HTTP basic auth without the need for a database. For Laravel 5",
    "keywords": [
        "olssonm",
        "Laravel 5",
        "authentication",
        "http basic auth"
    ],
    "homepage": "https://github.com/olssonm/l5-very-basic-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Olsson",
            "email": "contact@marcusolsson.me",
            "homepage": "https://marcusolsson.me"
        }
    ],
    "require": {
        "illuminate/support": "~5.4.0|~5.5.0|~5.6.0",
        "php" : "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || 7.1",
        "orchestra/testbench": "~3.4.0|~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Consilience\\ApiBasicAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Consilience\\ApiBasicAuth\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "laravel": {
            "providers": [
                "Consilience\\ApiBasicAuth\\ApiBasicAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
